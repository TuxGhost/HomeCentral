@model IEnumerable<Home_Central.Data.Entities.WoningVerbruikModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.datum)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GasStand)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DagTeller)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NachtTeller)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Zon)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.datum)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GasStand)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DagTeller)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NachtTeller)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Zon)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
